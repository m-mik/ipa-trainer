                  <a amp-access="loggedIn" amp-access-hide class="dwla wordlist-add-button" title="Add this meaning to a word list" on="tap:AMP.setState({ stateGlobal: { wlSenseId: 'CACD_00005127_01' } }), sidebarWordList.open">
