                [class]="stateSearch.autocomplete && stateSearchAutocomplete.length > 0 && stateHdr.searchDesk ? 'pa pdd  chac-sb  tc-bd bw hbr-20 hbss lpt-25' : 'hdn'">

                <div class="hax fs16 lmt-25 lmb-20 lpl-5 minz bw">
