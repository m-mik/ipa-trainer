{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/common/*": [
        "common/*"
      ],
      "@/data/*": [
        "common/data/*"
      ],
      "@/hooks/*": [
        "common/hooks/*"
      ],
      "@/hocs/*": [
        "common/hocs/*"
      ],
      "@/components/*": [
        "common/components/*"
      ],
      "@/utils/*": [
        "common/utils/*"
      ],
      "@/services/*": [
        "common/services/*"
      ],
      "@/middlewares/*": [
        "common/middlewares/*"
      ],
      "@/layouts/*": [
        "layouts/*"
      ],
      "@/modules/*": [
        "modules/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/prisma/*": [
        "prisma/*"
      ],
      "@/theme/*": [
        "theme/*"
      ],
      "@/public/*": [
        "public/*"
      ],
      "test-utils": [
        "common/utils/test-utils"
      ]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
